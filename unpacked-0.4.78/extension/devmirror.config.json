{
  "mode": "cdp",
  "outputDir": "./devmirror-logs",
  "url": "http://localhost:12358",
  "autoDetectPort": false,
  "autoOpenBrowser": true,
  "captureDeprecationWarnings": true,
  "captureViteErrors": true,
  "throttle": {
    "maxPerSecond": 100,
    "suppressAfter": 100
  },
  "lifecycle": {
    "captureNavigation": false,
    "captureSession": true,
    "capturePerformance": false,
    "captureDialogs": false,
    "capturePageNavigated": true,
    "capturePageLoaded": false,
    "captureDOMContentLoaded": false,
    "captureSessionStarted": true,
    "captureSessionEnded": true,
    "captureCEFConnected": true,
    "captureCEFDisconnected": true,
    "captureFirstPaint": false,
    "captureFirstContentfulPaint": false,
    "captureAlert": false,
    "captureConfirm": false,
    "captureWindowOpened": false
  },
  "captureJavaScriptErrors": true,
  "captureNetworkErrors": true,
  "captureSecurityErrors": true,
  "captureWebpackErrors": false,
  "captureTypeScriptErrors": false
}